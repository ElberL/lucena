#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <limits.h>

#include <math.h>
#include <string.h>
#include <ctype.h>
#include <time.h>

char MOD( char a)
{int d,i;
char b[25];
char nome[25];
char escrita[500];
double c;

	fflush(stdin);
	scanf("%[^\n] %lf", &b, &c);
 	d = strlen(b);
 	
		
	
	if (d>25) {
	
	for(i = 0; i<25 ;i++){
	
	nome[i] = b[i];
	}
	}else {
		for(i = 0; i<d ;i++){
	
	nome[i] = b[i];
		
	}
	while( d < 25){
		nome[d] = '1';
		d++;
		}
	
	}
	printf("%i %s %.2lf", a, nome, c);
	
	for (i = 0; i < 500; i++)
		
			if ( i = 0)
			{
			escrita[0] = a;
			}if ( i = 1)
			{
			escrita[1] = ' ';	
			}
			while ( nome[i] != '\n'){
			
			escrita[i]= nome[i];
			i++;
			}i++;
			escrita[i] = ' ';	
			i++;
			escrita[i] = c;
    	
			return escrita[i];
    
} 

int man()
{
	int a ;
	char n1[500];
	char resultado [500];
	int i;
	
	fflush(stdin);
    scanf("%i\n" , &a);
	
	if (a > 0){
	
	resultado[i] = MOD(n1[i]);
	
	printf ( "%s", resultado);
}
return 0;
}
