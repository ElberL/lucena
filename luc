#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <limits.h>

#include <math.h>
#include <string.h>
#include <ctype.h>
#include <time.h>

int main()
{
	char str[250];
	
	char letra[50];
	char impressao[5];
	
	int i =0 ,  c = 0, espaco =0, a = 0, nome = 0, t = 0, z = 0, k= 0, im= 0, teste = 0; 
	
	setbuf(stdin, NULL);
	
		fgets(str);
	
	a = strlen(str);//tamanho da streeng
	char str2[a];
	
		for(i = 0; i<a ;i++){
			
		 // percorre pela primeira vez
			if(str[i] == ' '){		// procura os espacos
				espaco = espaco + 1;
				}

		} 
		i =0;
		for(i = 0; i<a ;i++){ // percorre pela segunda vez preenchendorimeira vez
			
			if(str[i] == ' ')
			{ teste = teste + 1;
				if( teste < espaco){		// procura os espacos
				i++;
				letra[z] = ' ';
				z++;
				letra[z] = str[i];
				z++;
				letra[z] = '.';
				z++;
				 }
			}
			}
	 	i = 0 ;		// zerar para percorrer novamente
		for(i = 0; i<a ;i++){ //percorre pela terceira vez 
			if(str[i] == ' ') {
				nome = nome + 1;
					if ( nome == espaco){
						for(; i<a ;i++){ //percorre pela terceira vez 
							if(str[i] == ' ')
							{i++;
							}
								str2[t] = str[i];

								t++;
									if(str[i] == '\n'){ 
								
									nome = nome +1;	}
							}	
						}
			}	
		}
			i = 0 ;
			for(i = 0; i<1 ;i++){
				
				impressao[im] = ',';
				im++;
				impressao[im] = ' ';
				im++;
				impressao[im] = str[0];
				im++;
				impressao[im] = '.';
		}
	

			
			
			toupper(str2);
	strcat (str2,impressao);
	strcat (str2,letra);
	
	printf("%s", str2);
				
return 0;

}
	
	
